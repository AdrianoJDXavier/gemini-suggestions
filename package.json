{
  "name": "gemini-suggestions",
  "displayName": "gemini_suggestions",
  "description": "O Gemini Suggestions é uma extensão para Visual Studio Code que utiliza inteligência artificial para analisar seu código em tempo real, oferecendo sugestões de melhorias, otimizações e correções. ",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.86.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "gemini-suggestions.helloWorld",
        "title": "Hello World"
      }
    ],
    "keybindings": [
      {
        "command": "gemini-suggestions.helloWorld",
        "key": "ctrl+alt+g",
        "mac": "cmd+alt+g",
        "when": "true"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.86.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "eslint": "^8.57.0",
    "typescript": "^5.3.3",
    "@vscode/test-cli": "^0.0.8",
    "@vscode/test-electron": "^2.3.9"
  }
}
